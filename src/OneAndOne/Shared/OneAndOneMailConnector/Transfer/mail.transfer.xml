<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">

    <transfer name="Mail">
        <property name="schemaOrgData"
                  type="SchemaOrgData"/>
    </transfer>

    <transfer name="SchemaOrgData">
        <property name="parcelDeliveries"
                  type="ParcelDelivery[]"
                  singular="parcelDelivery"/>
        <property name="shopName"
                  type="string"/>
        <property name="status"
                  type="string"/>
    </transfer>
    
    <transfer name="ParcelDelivery">
        <property name="deliveryName"
                  type="string"/>
        <property name="status"
                  type="string"/>
        <property name="trackingNumber"
                  type="string"/>
    </transfer>
</transfers>

